{
  "name": "@watchtower/sdk",
  "version": "0.1.0",
  "description": "Simple game backend SDK - saves, multiplayer rooms, and more",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "game",
    "backend",
    "multiplayer",
    "websocket",
    "cloud-saves",
    "watchtower"
  ],
  "author": "Watchtower",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/watchtower-host/sdk"
  },
  "homepage": "https://watchtower.host",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
